{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "xo"],
	"overrides": [
		{
			"extends": ["xo-typescript"],
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"@typescript-eslint/naming-convention": "off",
				"no-empty-function": "off",
				"@typescript-eslint/no-empty-function": "off"
			}
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"]
}
